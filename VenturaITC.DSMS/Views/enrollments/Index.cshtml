@model IEnumerable<VenturaITC.DSMS.Models.enrollment>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.category.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.payment.id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.payment_type.name)
        </th>
