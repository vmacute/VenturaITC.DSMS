            @Html.DisplayFor(modelItem => item.payment.id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.payment_type.name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.student.full_name)
        </td>
